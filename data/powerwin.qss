/*睡眠提示界面的继续执行按钮*/
QPushButton#confirmBtn {
    background-color: rgb(255,255,255,80);
    border-style: outset;
    border-width: 0px;
    border-radius: 24px;
}

/*睡眠提示界面的取消按钮*/
QPushButton#cancelBtn {
    background-color: rgb(255,255,255,80);
    border-style: outset;
    border-width: 0px;
    border-radius: 24px;
}

/*提示界面的应用列表*/
QListView#applist {
    background-color: rgb(255,255,255,80);
    border-style: outset;
    border-width: 0px;
    border-radius: 4px;
}
